<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.PositionAddEditController">
   <children>
      <TextField fx:id="txtPosition" layoutX="24.0" layoutY="72.0" prefHeight="25.0" prefWidth="233.0" promptText="Позиция" />
      <CheckBox fx:id="chkActive" layoutX="24.0" layoutY="118.0" mnemonicParsing="false" prefHeight="34.0" prefWidth="119.0" text="Active" />
      <Button fx:id="btnSave" layoutX="24.0" layoutY="175.0" mnemonicParsing="false" onAction="#onButtonClicked" text="Сохранить" />
      <Button fx:id="btnCancel" layoutX="143.0" layoutY="175.0" mnemonicParsing="false" onAction="#onButtonClicked" text="Отмена" />
   </children>
</AnchorPane>
